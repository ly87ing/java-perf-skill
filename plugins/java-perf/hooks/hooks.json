{
    "hooks": {
        "SessionStart": [
            {
                "hooks": [
                    {
                        "type": "command",
                        "command": "bash \"${CLAUDE_PLUGIN_ROOT}/scripts/install-binary.sh\"",
                        "timeout": 120
                    },
                    {
                        "type": "command",
                        "command": "java-perf summary . --json 2>/dev/null || echo '{\"status\":\"binary_not_installed\"}'",
                        "timeout": 30
                    }
                ]
            }
        ]
    }
}
